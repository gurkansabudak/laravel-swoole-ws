{
  "name": "erfanvahabpour/laravel-swoole-ws",
  "description": "Laravel library for Swoole WebSocket Server with routing, channels, middleware, and broadcasting",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/console": "^10.0|^11.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0|^10.0",
    "phpunit/phpunit": "^10.0|^11.0"
  },
  "autoload-dev": {
    "psr-4": {
      "EFive\\Ws\\Tests\\": "tests/"
    }
  },
  "autoload": {
    "psr-4": { "EFive\\Ws\\": "src/" }
  },
  "extra": {
    "laravel": {
      "providers": [
        "EFive\\Ws\\WsServiceProvider"
      ],
      "aliases": {
        "WS": "EFive\\Ws\\Facades\\WS"
      }
    }
  }
}
